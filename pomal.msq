CREATE TABLE series ( sid UNSIGNED INT(8) NOT NULL PRIMARY KEY, sname VARCHAR(75) NOT NULL, lastwatched UNSIGNED INT(5) DEFAULT 0, started DATE, ended DATE, score UNSIGNED TINYINT, content UNSIGNED INT(35), rating UNSIGNED tinyint, lastrewatched UNSIGNED INT(5), seentimes UNSIGNED INT DEFAULT 0 );
CREATE TABLE episode ( sid UNSIGNED INT(8) NOT NULL, eid UNSIGNED INT(5) NOT NULL, ename VARCHAR(100), score UNSIGNED TINYINT, content UNSIGNED INT(35), rating UNSIGNED TINYINT );
CREATE TABLE pub ( pid UNSIGNED INT(8) NOT NULL PRIMARY KEY, pname VARCHAR(75) NOT NULL, lastread UNSIGNED INT(5) DEFAULT 0, started DATE, ended DATE, score UNSIGNED TINYINT, content UNSIGNED INT(35), rating UNSIGNED tinyint, lastreread UNSIGNED INT(5), readtimes UNSIGNED INT DEFAULT 0 );
CREATE TABLE volume ( pid UNSIGNED INT(8) NOT NULL, vid UNSIGNED INT(5) NOT NULL, vname VARCHAR(100), score UNSIGNED TINYINT, content UNSIGNED INT(35), rating UNSIGNED TINYINT );
CREATE TABLE chapter ( pid UNSIGNED INT(8) NOT NULL, vid UNSIGNED INT(5) NOT NULL, vid UNSIGNED INT(5) NOT NULL, cname VARCHAR(100), score UNSIGNED TINYINT, content UNSIGNED INT(35), rating UNSIGNED TINYINT );
CREATE TABLE tag ( tid UNSIGNED INT(10) NOT NULL PRIMARY KEY AUTO_INCREMENT, text VARCHAR(65) );
CREATE TABLE tags ( sid UNSIGNED INT(8), tid UNSIGNED INT(10) NOT NULL );
